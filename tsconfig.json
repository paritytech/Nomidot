{
  "exclude": [
    "**/generated/**/*",
    "node_modules",
    "public",
    ".cache",
    "back/server/**/*",
    "front/gatsby/**/*"
  ],
  "include": ["**/*.js", "**/*.ts", "**/*.tsx"],
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  }
}
