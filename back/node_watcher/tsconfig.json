{
  "exclude": ["**/generated/**/*", "node_modules", "public", ".cache"],
  "include": ["./src"],
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "outDir": "./lib",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  }
}
